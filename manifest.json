{
  "manifest_version": 2,

  "name": "Block External Script",
  "description": "Blocks external script",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "38": "enabled.png"
    },
    "default_title": "Block External Script"
  }
}
